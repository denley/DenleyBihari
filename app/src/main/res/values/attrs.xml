<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DrawerListItem">
        <attr name="dli_icon" format="reference"/>
        <attr name="dli_text" format="string"/>
    </declare-styleable>

    <declare-styleable name="TestimonialCardView">
        <attr name="tcv_quote" format="string"/>
        <attr name="tcv_author" format="string"/>
    </declare-styleable>

    <declare-styleable name="PortfolioCardView">
        <attr name="pcv_name" format="string"/>
        <attr name="pcv_client" format="string"/>
        <attr name="pcv_year" format="string"/>
        <attr name="pcv_url" format="string"/>
        <attr name="pcv_splash_image" format="reference"/>
        <attr name="pcv_icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ExperienceCardView">
        <attr name="ecv_role" format="string"/>
        <attr name="ecv_institution" format="string"/>
        <attr name="ecv_timeline" format="string"/>
        <attr name="ecv_type" format="enum">
            <enum name="education" value="0" />
            <enum name="experience" value="1" />
        </attr>
    </declare-styleable>

</resources>